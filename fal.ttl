# Properties for Method
#################################################################

fal:hasMethodTitle a rdf:Property ;
    rdfs:label "Title" ;
    rdfs:domain fal:Method ;
    rdfs:range xsd:string ;
    rdfs:comment "This should be a short, descriptive name that is unique to this method (not for the technique or tools!). It is good practice include the acronym of the institution, technique or equipment label and a date or version label. Example: 'NG Lux Allowance Method 2024'." .

fal:hasMethodType a rdf:Property ;
    rdfs:label "Method Type" ;
    rdfs:domain fal:Method ;
    rdfs:range xsd:string ;
    rdfs:comment "The type of method this represents. Examples are 'Analysis Method', 'Research support', 'Workflow'... This is a free text field but will be converted into a controlled vocabulary field." .

fal:hasCoreParameters a rdf:Property ;
    rdfs:label "Core Parameters" ;
    rdfs:domain fal:Method ;
    rdfs:range xsd:string ;
    rdfs:comment "This should be the technical specification for particular devices. It will become a new class in the future" .

# Relationship Properties

fal:hasMethodAuthor a rdf:Property ;
