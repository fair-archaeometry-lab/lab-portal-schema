#################################################################
# Universal Properties 
#################################################################

fal:hasIdentifier a rdf:Property ;
    rdfs:label "Identifiers" ;
    rdfs:range xsd:string ;
    rdfs:comment "Identifiers used to define the digital resource." .

dc:title a rdf:Property ;
    rdfs:label "Name" ;
    rdfs:range xsd:string ;
    rdfs:comment "The name of the digital resource." .

fal:hasDescription a rdf:Property ;
    rdfs:label "Description" ;
    rdfs:range xsd:string ;
    rdfs:comment "A description/documentation of the digital resource." .

fal:hasTechniques a rdf:Property ;
    rdfs:label "Analytical Techniques" ;
    rdfs:range xsd:string ;
    rdfs:comment "A list of the analytical techniques carried out within this service. The terms are taken from the Research Vocabularies Australia (https://vocabs.ardc.edu.au/viewById/650)." .

fal:hasDisciplines a rdf:Property ;
    rdfs:label "Research Disciplines" ;
    rdfs:range xsd:string ;
    rdfs:comment "Research disciplines associated with the digital resource." .

fal:hasVersion a rdf:Property ;
    rdfs:label "Version" ;
    rdfs:range xsd:string ;
    rdfs:comment "The version number of the digital resource" .

fal:hasKeywords a rdf:Property ;
    rdfs:label "Keywords" ;
    rdfs:range xsd:string ;
    rdfs:comment "Additional terms and concepts that highlight the digital resource." .

fal:hasRelatedTools a rdf:Property ;
    rdfs:label "Related Tools" ;
    rdfs:range xsd:string ;
    rdfs:comment "Tools (software or devices) that are related to this digital resource." .

fal:hasPublication a rdf:Property ;
    rdfs:label "Related Publications" ;
    rdfs:range fal:Publication ;
    rdfs:comment "References related to the digital resource" .

fal:hasOrganisation a rdf:Property ;
    rdfs:label "Organisations" ;
    rdfs:range fal:Organisation ;
    rdfs:comment "Information about the organisations that create or relate to the digital resource." .

fal:hasPerson a rdf:Property ;
    rdfs:label "People" ;
    rdfs:range fal:Person ;
    rdfs:comment "Information about people related to the digital contents." .

fal:hasRole a rdf:Property ;
    rdfs:label "Role" ;
    rdfs:range xsd:string ;
    rdfs:comment "What role does this person or organisation play in the creation of the digital resource" .

fal:hasLinks a rdf:Property ;
    rdfs:label "Related Links" ;
    rdfs:range xsd:anyURI ;
    rdfs:comment "The URLs for the related webpages, files, and other types of digital resources." .

fal:hasCountry a rdf:Property ;
    rdfs:label "Country of Operation" ;
    rdfs:range xsd:string ;
    rdfs:comment "The country in which the person is based for legal and professional reasons, the value will be used in relation to distribution of people and in relation to initial assessment of eligibility when it is related to location." .

fal:hasEmail a rdf:Property ;
    rdfs:label "Email" ;
    rdfs:range xsd:string ;
    rdfs:comment "Contact email address (professional)." .

fal:hasPhone a rdf:Property ;
    rdfs:label "Phone" ;
    rdfs:range xsd:string ;
    rdfs:comment "Contact phone number (professional)." .

fal:hasImage a rdf:Property ;
    rdfs:label "Image URL" ;
    rdfs:range xsd:anyURI ;
    rdfs:comment "URL to an image representing the person (e.g. portrait or profile photo)." .

fal:hasWebPage a rdf:Property ;
    rdfs:label "Webpage" ;
    rdfs:range xsd:anyURI ;
    rdfs:comment "Webpage(s) related to the person (e.g. personal webpage, profile page on the institutional website, etc.)." .

fal:hasJoinDate a rdf:Property ;
    rdfs:label "Date of joining the field" ;
    rdfs:range xsd:date ;
    rdfs:comment "When did the person or organisation start working in this field?" .

fal:hasCreationDate a rdf:Property ;
    rdfs:label "Creation Date" ;
    rdfs:range xsd:date ;
    rdfs:comment "The creation date of the digital resource." .



