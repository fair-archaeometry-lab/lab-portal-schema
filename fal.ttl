fal:isPartOf rdf:Property ;
    rdfs:label "Is Part Of" ;
    rdfs:domain fal:Software ;
    rdfs:range xsd:anyURI ;
    rdfs:comment "Indicates a CreativeWork that this CreativeWork is part of." .

fal:hasSoftwareInputDataType a rdf:Property ;
    rdfs:label "Input Data Type" ;
    rdfs:domain fal:Software ;
    rdfs:range xsd:string ;
