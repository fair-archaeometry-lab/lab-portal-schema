@prefix fal: <https://fair-archaeometry-lab.github.io/lab-portal-schema/#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix dc: <http://purl.org/dc/terms/> .
@prefix bibo: <http://purl.org/ontology/bibo/> .

#################################################################
# Ontology Definition
#################################################################

fal: a owl:Ontology ;
    rdfs:label "Metadata Profile for Fair Archaeometry Lab" ;
    rdfs:comment "A RDF version of metadata profile for fair archaeometry lab." ;
    owl:versionInfo "1.0" .

#################################################################
# Classes
#################################################################

fal:Service a rdfs:Class ;
    rdfs:label "Service" ;
    rdfs:comment "Service schema that defines analytical activities." .

fal:Device a rdfs:Class ;
    rdfs:label "Device" ;
    rdfs:comment "The schema that defines the analytical instruments. It complies with the profile of PIDINST working group" .

fal:Software a rdfs:Class ;
    rdfs:label "Software" ;
    rdfs:comment "Software model taken from https://codemeta.github.io/codemeta-generator/" .

fal:Organisation a rdfs:Class ;
    rdfs:label "Organisation" ;
    rdfs:comment "An actor offering this service, as defined in the system." .

fal:Method a rdfs:Class ;
    rdfs:label "Standard Method" ;
    rdfs:comment "The schema outlines the required metadata to define a standard method or protocol." .

fal:Person a rdfs:Class ;
    rdfs:label "Person" ;
    rdfs:comment "Person that involves in the service" .

fal:Publication a rdfs:Class ;
    rdfs:label "Publication" ;
    rdfs:comment "Publications related to the service" .

fal:ReferenceMaterial a rdf:Class ;
    rdfs:label "Reference Material" ;
    rdfs:comment "A reference material used for calibration, correction and quantification of analytical results." .

fal:Analyte a rdf:Class ;
    rdfs:label "Analyte" ;
    rdfs:comment "Certificate of reference materials." .

#################################################################
# Properties for Service
#################################################################

fal:hasServiceSummary a rdf:Property ;
    rdfs:label "Service Summary" ;
    rdfs:domain fal:Service ;
    rdfs:range xsd:string ;
    rdfs:comment "A brief description of a specific technical or access service offered, such as the use of X to investigate Y." .

fal:hasReadinessLevel a rdf:Property ;
    rdfs:label "Service Readiness Level" ;
    rdfs:domain fal:Service ;
    rdfs:range xsd:string ;
    rdfs:comment "This scale is directly based on the standard - Technology readiness levels (TRLs), adapted for heritage science, which are a method for estimating the maturity of the services being offered. For more details see https://en.wikipedia.org/wiki/Technology_readiness_level." .

fal:hasOperatingLanguages a rdf:Property ;
    rdfs:label "Operating Languages" ;
    rdfs:domain fal:Service ;
    rdfs:range xsd:string ;
    rdfs:comment "What languages can the team operate in or what language is a tool presented in." .

fal:hasMeasurableProperties a rdf:Property ;
    rdfs:label "Measurable Properties" ;
    rdfs:domain fal:Service ;
    rdfs:range xsd:string ;
    rdfs:comment "Measurable properties that can be studied by this service." . 

fal:hasMaterials a rdf:Property ;
    rdfs:label "Materials" ;
    rdfs:domain fal:Service ;
    rdfs:range xsd:string ;
    rdfs:comment "Materials that can be studied by this service." .

fal:hasResearchQuestions a rdf:Property ;
    rdfs:label "Research Questions" ;
    rdfs:domain fal:Service ;
    rdfs:range xsd:string ;
    rdfs:comment "Typical research questions that a service designed to answer. This field is free text, but will likely be replaced with a controlled list." .

fal:hasServiceFunctions a rdf:Property ;
    rdfs:label "Service Functions" ;
    rdfs:domain fal:Service ;
    rdfs:range xsd:string ;
    rdfs:comment "Short and descriptive service practical level activities, what has a service been used for, what is it intended for. E.g. 'Materials Analysis', 'Dye Analysis', 'De-convolution XRF Spectra', 'Calculating Light Exposure Allowance'. At the moment, these are free text but will likely be replaced with a controlled list." .

fal:hasLimitations a rdf:Property ;
    rdfs:label "Service Limitations" ;
    rdfs:domain fal:Service ;
    rdfs:range xsd:string ;
    rdfs:comment "Description of any limitations in the access provision by the service." .

fal:hasServiceResources a rdf:Property ;
    rdfs:label "Research Resources" ;
    rdfs:domain fal:Service ;
    rdfs:range xsd:string ;
    rdfs:comment "Research resources made accessible by the service should be defined in the system and represent the heritage collections, objects, samples, archives, other research resources directly provided by the service." .

fal:hasInputDatasets a rdf:Property ;
    rdfs:label "Example input datasets" ;
    rdfs:domain fal:Service ;
    rdfs:range xsd:string ;
    rdfs:comment "Services that process data can provide PIDs/URLs for example datasets to demonstrate what data and formats are required for the Service to run." .

fal:hasOutputDatasets a rdf:Property ;
    rdfs:label "Example output datasets" ;
    rdfs:domain fal:Service ;
    rdfs:range xsd:string ;
    rdfs:comment "Services that generate data can provide PIDs/URLs for example datasets to demonstrate what types of data and information will be created." .

fal:hasServiceVersionDate a rdf:Property ;
    rdfs:label "Service Version Date" ;
    rdfs:domain fal:Service ;
    rdfs:range xsd:date ;
    rdfs:comment "The version date is the date from which this particular version of this Service was offered in a formal capacity by the defined Access provider." .

fal:hasServiceDMP a rdf:Property ;
    rdfs:label "Service Data Management Plan" ;
    rdfs:domain fal:Service ;
    rdfs:range xsd:string ;
    rdfs:comment "An open description of the data management procedures followed with the defined service, including licence issues, preferred data repositories, publication embargoes, common file formats, software and systems used, etc." .

fal:hasServiceDataflow a rdf:Property ;
    rdfs:label "Service Data Management Plan" ;
    rdfs:domain fal:Service ;
    rdfs:range xsd:string ;
    rdfs:comment "A reference to a diagram or written description of the dataflow. More complex services, multiple pieces of equipment or data processing steps, might benefit for a summary description of the key stages, including details of any required background information, direct input from users and any processing steps." .

fal:hasServiceOutputDescription a rdf:Property ;
    rdfs:label "Service Output Description" ;
    rdfs:domain fal:Service ;
    rdfs:range xsd:string ;
    rdfs:comment "A description of all of the raw and processes outputs created by a given services including details of how they relate to or rely on each other." .

fal:hasServiceInputDescription a rdf:Property ;
    rdfs:label "Service Input Description" ;
    rdfs:domain fal:Service ;
    rdfs:range xsd:string ;
    rdfs:comment "A description of the required formats or files required for any service which processes existing data." .

fal:hasServiceOtherComments a rdf:Property ;
    rdfs:label "Other Comments" ;
    rdfs:domain fal:Service ;
    rdfs:range xsd:string ;
    rdfs:comment "Free text opportunity to advocate for what is novel or special about this particular access." .

fal:hasAccessPeriodUnit a rdf:Property ;
    rdfs:label "Service Access Period Unit" ;
    rdfs:domain fal:Service ;
    rdfs:range xsd:string ;
    rdfs:comment "For previously defined services this has commonly been defined in days or occasionally hours - but with the development of digital services additional smaller time units might be needed. The possibility of having the costs 'per sample' is currently being evaluated. For the time being, please calculate your service costs in terms of time units." .

fal:hasAccessHourUnit a rdf:Property ;
    rdfs:label "Service Access Hours per Unit" ;
    rdfs:domain fal:Service ;
    rdfs:range xsd:integer ;
    rdfs:comment "Access units are described as time periods, but depending on the nature of a service this might relate to a range of different periods of activity time. For example a day could relate to a working day or a calendar day, a digital service could relate to run time or processing time. So this value is used to relate actual activity time to the recorded access unit. This value is only required in case of ambiguous access unit types. Use '0' if not applicable." .

fal:hasAccessUnitCost a rdf:Property ;
    rdfs:label "Service Access Unit Cost" ;
    rdfs:domain fal:Service ;
    rdfs:range xsd:decimal ;
    rdfs:comment "Service offered can include a unit cost in Euro. This cost is expected to represent a proportion of the actual cost of providing a given Service ranging from zero to the full actual cost of providing the service. Access service costs will be based on potentially discounted actual costs and may not include any for-profit contributions." .

fal:hasActiveService a rdf:Property ;
    rdfs:label "Active Service" ;
    rdfs:domain fal:Service ;
    rdfs:range xsd:boolean ;
    rdfs:comment "Is the service currently open for access?" .


# Relationship Properties

fal:hasServiceMethods a rdf:Property ;
    rdfs:label "Method Statements" ;
    rdfs:domain fal:Service ;
    rdfs:range fal:Method ;
    rdfs:comment "Service related method statements should be defined in the system and represent the procedures for using individual pieces of equipment or software systems used within the service - detail of setup, methodology, procedures, etc. should all be described within this document and is also directly attached to relevant the Tool (equipment or software) records. Linked to the method model." .

#################################################################
# Properties for Device
#################################################################

fal:hasDeviceLandingPage a rdf:Property ;
    rdfs:label "Landing Page" ;
    rdfs:domain fal:Device ;
    rdfs:range xsd:anyURI ;
    rdfs:comment "A landing page that the identifier resolves to." .

fal:hasDeviceModelName a rdf:Property ;
    rdfs:label "Model Name" ;
    rdfs:domain fal:Device ;
    rdfs:range xsd:string ;
    rdfs:comment "Name of the model or type of device as attributed by the manufacturer." .

fal:hasDeviceModelIdentifier a rdf:Property ;
    rdfs:label "Model Identifier" ;
    rdfs:domain fal:Device ;
    rdfs:range xsd:string ;
    rdfs:comment "Identifier used to identify the model." .

fal:hasInstrumentTypeName a rdf:Property ;
    rdfs:label "Instrument Type" ;
    rdfs:domain fal:Device ;
    rdfs:range xsd:string ;
    rdfs:comment "Classification of the type of the instrument." . 

fal:hasMeasuredVariable a rdf:Property ;
    rdfs:label "Measured Variables" ;
    rdfs:domain fal:Device ;
    rdfs:range xsd:string ;
    rdfs:comment "The variable(s) that this instrument measures or observes." . 

fal:hasCommissionDated a rdf:Property ;
    rdfs:label "Commission Date" ;
    rdfs:domain fal:Device ;
    rdfs:range xsd:date ;
    rdfs:comment "The date when the instrument started to operate." .

fal:hasDeCommissionDated a rdf:Property ;
    rdfs:label "DeCommission Date" ;
    rdfs:domain fal:Device ;
    rdfs:range xsd:date ;
    rdfs:comment "The date when the instrument retired." .

fal:hasRelatedIdentifier a rdf:Property ;
    rdfs:label "Related Identifiers" ;
    rdfs:domain fal:Device ;
    rdfs:range xsd:string ;
    rdfs:comment "Identifiers of related resources." .

fal:hasRelationType a rdf:Property ;
    rdfs:label "Relation Type" ;
    rdfs:domain fal:Device ;
    rdfs:range xsd:string ;
    rdfs:comment "Description of the relationship." .

fal:hasRelatedIdentifierName a rdf:Property ;
    rdfs:label "Name of Related Identifier" ;
    rdfs:domain fal:Device ;
    rdfs:range xsd:string ;
    rdfs:comment "A name for the related resource, may be used to give a hint on the content of that resource." .

fal:hasAlternateIdentifier a rdf:Property ;
    rdfs:label "Alternate Identifiers" ;
    rdfs:domain fal:Device ;
    rdfs:range xsd:string ;
    rdfs:comment "Identifiers other than the PIDINST pertaining to the same instrument instance. This should be used if the instrument has a serial number. Other possible uses include an owner's inventory number or an entry in some instrument data base." .

fal:hasAlternateIdentifierName a rdf:Property ;
    rdfs:label "Name of Alternate Identifier" ;
    rdfs:domain fal:Device ;
    rdfs:range xsd:string ;
    rdfs:comment "A name for the related resource, may be used to give a hint on the content of that resource." .

fal:hasLastCheckDate a rdf:Property ;
    rdfs:label "Last Checked Date" ;
    rdfs:domain fal:Device ;
    rdfs:range xsd:date ;
    rdfs:comment "For hardware tools the last checked date relates to when equipment was last tested or calibrated, to confirm that it is working and performing as designed." .

fal:hasAcquisitionArea a rdf:Property ;
    rdfs:label "Acquisition Area" ;
    rdfs:domain fal:Device ;
    rdfs:range xsd:string ;
    rdfs:comment "The areas of the object or sample that can be examined by the equipment. This dropdown list gives a few examples, but if required, you can enter a different term here depending on the equipment. One general recommendation is to extend the existing term with a precise value or a range (e.g. 'small spot (27mm2)')." .

fal:hasImpact a rdf:Property ;
    rdfs:label "Impact on object or sample" ;
    rdfs:domain fal:Device ;
    rdfs:range xsd:string ;
    rdfs:comment "The impact of the equipment on the object being examined." .

fal:hasLastCalibrated a rdf:Property ;
    rdfs:label "Last Calibrated" ;
    rdfs:domain fal:Device ;
    rdfs:range xsd:date ;
    rdfs:comment "Date when the equipment was last calibrated." .

# Relationship Properties

fal:hasDeviceManufacturer a rdf:Property ;
    rdfs:label "Manufacturer" ;
    rdfs:domain fal:Device ;
    rdfs:range [ a owl:Class ;
                owl:unionOf (fal:Organisation fal:Person) ] ;
    rdfs:comment "The instrument's manufacturer(s) or developer. This may also be the owner for custom build instruments." .

fal:hasDeviceOwner a rdf:Property ;
    rdfs:label "Owner" ;
    rdfs:domain fal:Device ;
    rdfs:range [ a owl:Class ;
                owl:unionOf (fal:Organisation fal:Person) ] ;
    rdfs:comment "Institution(s) responsible for the management of the instrument. This may include the legal owner, the operator, or an institute providing access to the instrument." .

